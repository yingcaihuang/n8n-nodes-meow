{
  "name": "n8n-nodes-meow",
  "version": "0.1.5",
  "description": "n8n node for sending notifications via MeoW API",
  "keywords": [
    "n8n",
    "n8n-nodes",
    "n8n-community-node",
    "meow",
    "notification",
    "push",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yingcaihuang/n8n-nodes-meow#readme",
  "author": {
    "name": "yingcaihuang",
    "email": "b13621367396@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yingcaihuang/n8n-nodes-meow.git"
  },
  "bugs": {
    "url": "https://github.com/yingcaihuang/n8n-nodes-meow/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes/**/*.ts credentials/**/*.ts --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts --ext .ts",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts --ext .ts --fix",
    "check-publish": "node scripts/check-publish.js",
    "prepublishOnly": "npm run build && npm run check-publish"
  },
  "files": [
    "dist",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MeoWApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MeoW/MeoW.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-nodes-meow": "^0.1.1"
  }
}
